{
  "name": "english-accents-map",
  "version": "0.1.0",
  "description": "A map of English accents",
  "main": "index.js",
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "build": "better-npm-run build",
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "deploy": "better-npm-run deploy",
    "lint": "eslint bin build config server src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "start:dev": "better-npm-run start:dev",
    "start:prod": "better-npm-run start:prod",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "build": {
      "command": "npm run clean && npm run compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server --ignore dist --ignore coverage --ignore tests --ignore src",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run build && firebase deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start:dev": {
      "command": "node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shakacu/React-PWA-Map-App.git"
  },
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.26.0",
    "better-npm-run": "0.0.11",
    "css-loader": "^1.0.1",
    "cssnano": "^4.0.0",
    "debug": "^2.6.9",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "firebase": "^7.2.0",
    "fs-extra": "^0.30.0",
    "google-maps-react": "^1.1.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "json-loader": "^0.5.7",
    "material-design-lite": "^1.2.1",
    "node-sass": "^3.7.0",
    "normalize.css": "^4.1.1",
    "postcss-loader": "^0.13.0",
    "prop-types": "^15.6.0",
    "react": "^15.6.2",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.6.2",
    "react-ga": "^2.4.1",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-persist": "^4.10.2",
    "redux-thunk": "^2.0.0",
    "reselect": "^2.5.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^4.0.0",
    "sass-material-colors": "0.0.5",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.14",
    "yargs": "^5.0.0",
    "youtube-player": "^3.0.5"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-istanbul": "^2.0.1",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "chai-enzyme": "^0.5.0",
    "cheerio": "^0.20.0",
    "codecov": "^3.6.5",
    "connect-history-api-fallback": "^1.5.0",
    "enzyme": "^2.0.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.0.0",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "express": "^4.16.2",
    "karma": "^3.1.4",
    "karma-coverage": "^2.0.0",
    "karma-mocha": "^1.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.0",
    "mocha": "^4.0.0",
    "nodemon": "^1.15.0",
    "react-addons-test-utils": "^15.6.2",
    "redbox-react": "^1.5.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.14.0",
    "sw-precache-webpack-plugin": "^0.7.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  }
}
